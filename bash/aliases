# Generic
function take {
	mkdir $1
	cd $1
}

function c {
	cd ~/code/$1
}

function railsapp {
	template=$1
	appname=$2
	shift 2
	rails $appname -m http://github.com/olkarls/rails-templates/raw/master/$template.rb $@	
}

# Rails
alias sc='./script/console'
alias sg='./script/generate'
alias sp='./script/plugin'
alias ss='./script/server'
alias rmig='rake db:migrate'